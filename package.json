{
  "name": "vue-gh-corners",
  "version": "3.0.1",
  "description": "GitHub Corners for Vue.",
  "main": "./dist/vue-github-corners.cjs.js",
  "module": "./dist/vue-github-corners.esm.mjs",
  "unpkg": "./dist/vue-github-corners.iife.min.js",
  "jsdelivr": "./dist/vue-github-corners.iife.min.js",
  "style": "./dist/vue-github-corners.iife.min.css",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "del-cli dist/",
    "build": "bili",
    "predev": "del-cli dev-dist/",
    "dev": "parcel index.html -d dev-dist/ --open --no-autoinstall",
    "prepublishOnly": "npm run build",
    "lint": "tslint -p .",
    "test": "npm run lint && cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/vue-gh-corners.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue-component",
    "github",
    "corners"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/vue-gh-corners/issues"
  },
  "homepage": "https://github.com/gluons/vue-gh-corners",
  "browserslist": [
    "extends browserslist-config-vue"
  ],
  "peerDependencies": {
    "vue": "2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/node": "12",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.4.9",
    "bili": "^5.0.5",
    "browserslist-config-vue": "^1.0.3",
    "cross-env": "^5.2.0",
    "del-cli": "^3.0.1",
    "jest": "^26.2.2",
    "parcel-bundler": "^1.12.4",
    "rollup": "^2.23.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-vue": "5",
    "tsconfig-gluons": "^1.0.1",
    "tslint": "^5.13.1",
    "tslint-config-gluons": "^2.1.0",
    "typescript": "^3.3.3333",
    "vue": "2.6.11",
    "vue-hot-reload-api": "^2.3.4",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "tslib": "^2.0.0",
    "vue-runtime-helpers": "^1.1.2"
  }
